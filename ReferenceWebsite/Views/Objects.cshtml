@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using UmbracoVault

@{
    const int PAUL_ID = 1078;
    Layout = null;

    var paul = Vault.Context.GetContentById<ReferenceWebsite.Models.StaffMember>(PAUL_ID);
	var dates = Vault.Context.GetCurrent<ReferenceWebsite.Models.DateTimes>();
}

@Html.Partial("breadcrumbs")

<table border=1>
	<thead>
		<tr><th colspan=3>Arbitrary Objects</th></tr>
		<tr>
			<th>Type</th>
			<th>Expected</th>
			<th>Actual</th>
		</tr>
	</thead>
	<tbody>
		<tr><td>Object</td><td>Minneapolis</td><td>@paul.PrimaryLocation.Name</td></tr>
	</tbody>
</table>
	
<table border=1>
	<thead>
		<tr><th colspan=3>DateTime instances</th></tr>
		<tr>
			<th>Type</th>
			<th>Expected</th>
			<th>Actual</th>
		</tr>
	</thead>
	<tbody>
		<tr><td>DateTime From Date Picker</td><td>7/21/2010 3:43:00 PM</td><td>@dates.DateFromPicker</td></tr>
		<tr><td>DateTime From Textstring</td><td>7/21/1983 1:33:00 PM</td><td>@dates.DateFromText</td></tr>
	</tbody>
</table>